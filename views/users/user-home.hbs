{{!-- for laptop --}}
<div id="carouselExampleIndicators" class="carousel slide carousel-fade d-none d-md-block" data-ride="carousel">
    <ol class="carousel-indicators">
        {{#each bannerTop_main}}
        <li data-target="#carouselExampleIndicators" data-slide-to={{@index}}></li>
        {{/each }}
        <li data-target="#carouselExampleIndicators" data-slide-to="" class="active"></li>
    </ol>
    <div class="carousel-inner">
        {{#each bannerTop_main}}
        <div class="carousel-item ">
            <img class="d-block w-100" src="/images/banner-img/{{this.largeImg}}" alt="First slide">
            <div class="carousel-caption d-none d-md-block">
                <div>

                    <h2>{{this.bannerTitle}}</h2>
                    <p>{{this.bannerDescription}}</p>

                </div>

            </div>
        </div>
        {{/each }}
        <div class="carousel-item active">
            <img class="d-block w-100" src="/images/banner-img/lapend.jpg" alt="Third slide">
            <div class="carousel-caption d-none d-md-block">
                <h5> </h5>
                <p></p>
            </div>
        </div>
    </div>


    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>
{{!-- for small devices --}}
<div id="carouselExampleIndicatorsmob" class="carousel slide carousel-fade d-block d-md-none " data-ride="carousel">
